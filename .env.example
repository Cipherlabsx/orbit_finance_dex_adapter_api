# Server
PORT=8080
LOG_LEVEL=info

# Optional CORS allowlist
# Comma-separated origins
# Example: https://geckoterminal.com,https://yourdomain.com
CORS_ORIGINS=

# Solana / Orbit
# REQUIRED - use a dedicated RPC in production
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# REQUIRED - Orbit Finance program id
ORBIT_PROGRAM_ID=Fn3fA3fjsmpULNL7E9U79jKTe1KHxPtQeWdURCbJXCnM

# Identifier returned by /dex
DEX_KEY=orbit_finance

# Pool selection
# Optional static pool allowlist
# If empty, pools are discovered automatically on-chain
# Example:
# POOLS=FhZeGvu7oqJWGYGRkHzzFv1typcWzTJkeLP4XF74s8UQ,6mWxcL3ywYXSbHPA5NCd6FMoXTPVsMt9vmz43qegH4UW
POOLS=

# Enable on-chain pool discovery
DISCOVER_POOLS=true

# How often pool discovery runs (seconds)
DISCOVERY_REFRESH_SEC=300

# Max pools to discover per cycle
DISCOVERY_LIMIT=2000

# Indexer / Trades
# How many recent signatures to scan per pool
SIGNATURE_LOOKBACK=200

# Poll interval for trade indexing (milliseconds)
TRADES_POLL_MS=4000

# Caching
# In-memory cache TTL for pool reads (milliseconds)
CACHE_TTL_MS=2500

# Streamflow vault backfill limit
MAX_SIGS_PER_VAULT=100000

# Backfill (historic)
BACKFILL_MAX_SIGNATURES_PER_POOL=5000
BACKFILL_PAGE_SIZE=500

# DB 
SUPABASE_URL=""
SUPABASE_SERVICE_ROLE_KEY=""

# REST base URLs
HTTP_BASE_DEV=http://localhost:8080
HTTP_BASE_PROD=https://api.address.market

# WS base URLs (for clients, not server)
WS_BASE_DEV=ws://localhost:8080
WS_BASE_PROD=wss://api.orbit.finance

# WS auth (cli command: openssl rand -hex 128)
WS_TOKEN=super-secret-long-token-paste-here